n = int(input("Enter number from 0~255: "))
l = []
while n != 0:
    r = n % 16
    if r == 10:
        r = "A"
    elif r == 11:
        r = "B"
    elif r == 12:
        r = "C"
    elif r == 13:
        r = "D"
    elif r == 14:
        r = "E"
    elif r == 15:
        r = "F"
    l.append(r)
    n = n // 16

l.reverse()
print(l)
